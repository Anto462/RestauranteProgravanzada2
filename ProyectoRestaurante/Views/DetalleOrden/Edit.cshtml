@model ProyectoRestaurante.Models2.DetalleOrden


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Edit</title>
</head>
<body>

<h4>DetalleOrden</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
