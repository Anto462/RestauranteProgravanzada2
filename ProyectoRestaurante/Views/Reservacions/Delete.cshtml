@model ProyectoRestaurante.Models2.Reservacion

@{
    ViewData["Title"] = "Delete";
}

<h1 style="text-shadow: -1px 0px 6px #D4AF37;">Eliminar</h1>


<h3>¿Deseas proceder?</h3>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdReservacion" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-dark">Volver</a>
</form>
